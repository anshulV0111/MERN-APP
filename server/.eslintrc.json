{
   
    "extends": [
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "prettier/prettier": ["error", {"semi": false}],
        "semi": [2, "never"],
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off"
        
    }
}
